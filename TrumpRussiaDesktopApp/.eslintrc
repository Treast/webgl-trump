{
    "extends": "airbnb",
    "parser": "typescript-eslint-parser",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 2,
        "no-trailing-spaces": ["warn", "ignore-comments"]
    },
    "env": {
        "browser": true
    }
}
